{% extends 'back_end_base.html.twig' %}

{% block title %}{{parent()}}{{title}}{% endblock %}
{% block stylesheets %}
{{parent()}}
{% endblock %}

{% block body %}
<div class="row">
    <div class="col-4"></div>
    <div class="col-5">
        <div class="px-5 py-4 shadow">
            {{ form_start(form) }}
            <div class="form-outline mb-5">
                {{ form_row(form.name) }}
                {{ form_row(form.code) }}
            </div>
            <div class="row mt-3">
                <div class="col-3">
                    <button type="submit" class="btn btn-primary">{{btn}}</button>
                </div>
            </div>
            {{ form_rest(form) }}
            {{ form_end(form) }}
        </div>

    </div>
</div>
</div>
{% endblock %}