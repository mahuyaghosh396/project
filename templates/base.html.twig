<!DOCTYPE html>
<html>

<head>
	<meta charset="UTF-8">
	<title>
		{% block title %}Web-{% endblock %}
	</title>
	<script src="https://code.jquery.com/jquery-3.6.0.min.js"
		integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
	{% block stylesheets %}
	<link rel="stylesheet" type="text/css" href="/css/style.css">
	{% endblock %}
	{% block bootstrap %}
	 <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">    
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css"
		integrity="sha384-zCbKRCUGaJDkqS1kPbPd7TveP5iyJE0EjAuZQTgFLD2ylzuqKfdKlfG/eSrtxUkn" crossorigin="anonymous">
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"
		integrity="sha384-fQybjgWLrvvRgtW6bFlB7jaZrFsaBXjsOMm/tB9LTS58ONXgqbR9W8oWht/amnpF"
		crossorigin="anonymous"></script>
	{% endblock %}
</head>

<body>
	<div class="px-5 py-2 header">
		<div class="row align-items-center">
			<div class="col-2">
				<a class="logo" href="/">
					<img src="/images/WBSCTE.png" alt="">
				</a>
			</div>
			<div class="col-8">
				<div class="h2 text-center">
					Women’s Polytechnic Chandernagore <br>(Govt. of WB), Hooghly
				</div>
			</div>
		</div>
	</div>
	<nav class="navbar navbar-expand-lg py-0">

		<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
			aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon"></span>
		</button>
		<div class="collapse navbar-collapse justify-content-center" id="navbarNav">
			<ul class="navbar-nav">
				<li class="nav-item mx-2 active">
					<a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
				</li>
				<li class="nav-item mx-2 position-relative">
					<a class="nav-link" href="javascript:void(0)">About Us</a>
					<ul class="sub-menu list-unstyled ">
						<li>
							<a class="" href="{{path('web_college')}}">About College</a>
						</li>
						<li>							
							<a class="" href="{{path('web_gallery')}}">Gallery</a>
						</li>
					</ul>
				</li>
				<li class="nav-item mx-2 position-relative">
					<a class="nav-link" href="#">Programmes</a>
					<ul class="sub-menu list-unstyled ">
						<li>
							<a class="" href="#">ABC</a>
						</li>
						<li>
							<a class="" href="#">ABC</a>
						</li>
						<li>
							<a class="" href="#">ABC</a>
						</li>
					</ul>
				</li>
				<li class="nav-item mx-2">
					<a class="nav-link" href="#">Admissions</a>
				</li>
				<li class="nav-item mx-2">
					<a class="nav-link" href="#">Academics</a>
				</li>
				<li class="nav-item mx-2 position-relative">
					<a class="nav-link" href="#">News & Events</a>
					<ul class="sub-menu list-unstyled ">
						<li>
							<a class="" href="#">ABC</a>
						</li>
						<li>
							<a class="" href="#">ABC</a>
						</li>
						<li>
							<a class="" href="#">ABC</a>
						</li>
					</ul>
				</li>
				<li class="nav-item mx-2">
					<a class="nav-link" href="#">Tenders</a>
				</li>
				<li class="nav-item mx-2">
					<a class="nav-link" href="{{path('web_contact_us')}}">Contact Us</a>
				</li>
				<li class="nav-item mx-2 position-relative">
					<a class="nav-link" href="#">Act & Rules</a>
					<ul class="sub-menu list-unstyled ">
						<li>
							<a class="" href="#">ABC</a>
						</li>
						<li>
							<a class="" href="#">ABC</a>
						</li>
						<li>
							<a class="" href="#">ABC</a>
						</li>
					</ul>
				</li>
				{% if app.user %}
				<li class="nav-item mx-2 position-relative">
					<a class="nav-link" href="/logout">Logout</a>
				</li>
				{% else %}
				<li class="nav-item mx-2 position-relative">
					<a class="nav-link" href="/login">Login</a>
				</li>
				 {% endif %} 
			</ul>
		</div>
	</nav>
	<div class="row">
		<div class="col-12">
			{% for flashMessage in app.session.flashbag.get('successmsg') %}
			<div class="alert alert-success alert-dismissible" role="alert">
				<strong>
					<i class="fa fa-check scgreen"></i>
					Success!</strong>
				{{ flashMessage }}
			</div>
			{% endfor %}
			{% for flashMessage in app.session.flashbag.get('errormsg') %}
			<div class="alert alert-danger alert-dismissible" role="alert">
				<strong>
					<i class="fa fa-check scred"></i>
					Error!</strong>
				{{ flashMessage }}
			</div>
			{% endfor %}
		</div>
	</div>

	{% block slider %}{% endblock %}
	<div class="container-fluid">
		{% block body %}{% endblock %}
	</div>

	<footer class="footer mt-4 p-4">
		<h3 class="links-head mb-5 text-center">Explore More About Us</h3>
		<div class="row">
			<div class="col-12 col-md-4">
				<div class="h5 links-head mb-4">Institution Info</div>
				<ul class="list-unstyled">
					<li class="mb-1">
						<a class="text-white" href="#">Mission & Vision</a>
					</li>
					<li class="mb-1">
						<a class="text-white" href="#">History Of Institution</a>
					</li>
					<li class="mb-1">
						<a class="text-white" href="#">Organisational Structure</a>
					</li>
					<li class="mb-1">
						<a class="text-white" href="#">Hostel</a>
					</li>
					<li class="mb-1">
						<a class="text-white" href="#">Student Association</a>
					</li>
					<li class="mb-1">
						<a class="text-white" href="#">Library</a>
					</li>
				</ul>
			</div>
			<div class="col-12 col-md-4">
				<div class="h5 links-head mb-3">Academics</div>
				<ul class="list-unstyled">
					<li class="mb-1">
						<a class="text-white" href="#">Academic calender</a>
					</li>
					<li class="mb-1">
						<a class="text-white" href="#">Syllabus & Guideline</a>
					</li>
					<li class="mb-1">
						<a class="text-white" href="#">National Digital Library</a>
					</li>
					<li class="mb-1">
						<a class="text-white" href="#">Mission & Vision</a>
					</li>
					<li class="mb-1">
						<a class="text-white" href="#">Mission & Vision</a>
					</li>
				</ul>
			</div>
			
			<div class="col-12 col-md-4">
				<div class="h5 links-head mb-3">Contact Details</div>
				<ul class="list-unstyled">
					<li class="mb-1">
						<a class="text-white bi bi-geo-alt-fill style='color: white'" href="#">  Chandannagar Road, Near National Trading Company, Barabazar, Chandannagar, Kolkata, West Bengal 712136 · ~33.7 km</a>
					</li>
					<li class="mb-1">
						<a class="text-white  bi bi-telephone-fill style='color: white' "href="#">   033 24736448</a>
					</li>
					<li class="mb-1">
						<a class="text-white bi bi-envelope-fill style='color: white'" href="#">  wpc@gmail.com</a>
					</li>
				</ul>
			</div>
		</div>
	</footer>
	{% block javascripts %}
		<script src="/js/index.js"></script>
	{% endblock %}
</body>

</html>